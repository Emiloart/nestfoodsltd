{
  "name": "nestfoodsltd-platform",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter @nestfoodsltd/web dev",
    "build": "pnpm --filter @nestfoodsltd/web build",
    "start": "pnpm --filter @nestfoodsltd/web start",
    "lint": "pnpm --filter @nestfoodsltd/web lint",
    "typecheck": "pnpm --filter @nestfoodsltd/web typecheck",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.5.2"
  }
}
